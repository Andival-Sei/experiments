{
  "name": "experiments",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "pnpm lint:ts && pnpm lint:styles",
    "lint:staged": "lint-staged",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:styles:fix": "stylelint \"src/**/*.scss\" --fix",
    "lint:ts": "eslint . --max-warnings=0",
    "lint:ts:fix": "eslint . --fix",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest run",
    "test:all": "pnpm test && pnpm test:e2e --reporter=line",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:ui": "playwright test --ui",
    "test:watch": "vitest",
    "typecheck": "tsc --project tsconfig.app.json --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "pnpm exec eslint --max-warnings=0 --fix"
    ],
    "*.scss": [
      "pnpm exec stylelint --fix"
    ],
    "*": [
      "pnpm exec prettier --cache --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@dr.pogodin/react-helmet": "3.0.4",
    "mobx": "^6.15.0",
    "mobx-react-lite": "^4.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.55.1",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.13",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-testing-library": "^7.10.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.1",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "sass": "^1.93.2",
    "stylelint": "^16.24.0",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "npm:rolldown-vite@7.1.12",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.12"
    }
  }
}
